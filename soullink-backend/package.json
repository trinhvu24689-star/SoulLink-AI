{
  "name": "soullink-backend",
  "version": "1.0.0",
  "description": "Backend server for SoulLink AI with Neon DB and Prisma 7",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "express",
    "prisma",
    "neon-db",
    "soullink-ai"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "prisma": "5.22.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}